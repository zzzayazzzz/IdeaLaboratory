<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="kz.idealaboratory.idealaboratory.TechDetailActivity">

        <TextView
        android:id="@+id/tech_detail_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="5dp"
        tools:text="name"/>
        <TextView
            android:id="@+id/tech_hourly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tech_detail_name"
            android:layout_marginTop="5dp"
            tools:text="hourly price" />

        <TextView
            android:id="@+id/tech_shift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tech_hourly"
            android:layout_marginTop="5dp"
            tools:text="shift price" />

        <TextView
            android:id="@+id/tech_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tech_shift"
            android:layout_marginTop="5dp"
            tools:text="count" />
</LinearLayout>
